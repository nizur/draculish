{
  "name": "draculish",
  "version": "0.0.2",
  "description": "A theme for Logseq",
  "main": "src/index.scss",
  "scripts": {
    "build": "sass src/themes/theme-dark.scss:dist/theme-dark.css src/themes/theme-light.scss:dist/theme-light.css",
    "build:production": "npm run build -- --style compressed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nizur/draculish.git"
  },
  "keywords": [
    "css",
    "theme",
    "logseq"
  ],
  "author": "Chris Ruzin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nizur/draculish/issues"
  },
  "homepage": "https://github.com/nizur/draculish#readme",
  "devDependencies": {
    "sass": "^1.32.8"
  },
  "logseq": {
    "themes": [
      {
        "name": "Draculish Light",
        "url": "./dist/theme-light.css",
        "description": "Draculish in the day",
        "mode": "light"
      },
      {
        "name": "Draculish Dark",
        "url": "./dist/theme-dark.css",
        "description": "Draculish at night",
        "mode": "dark"
      }
    ],
    "id": "nizur_theme_draculish",
    "icon": "./icon.png"
  }
}
